{
  "name": "vue-admin-template",
  "version": "0.1.0",
  "description": "A vue admin template with Element UI & axios & iconfont & permission control & lint",
  "author": "Chan_ <jasonchenzehui@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build:prod": "vite build",
    "build:stage": "vite build --mode staging",
    "preview": "vite preview",
    "release": "release-it && git push --follow-tags origin master"
  },
  "dependencies": {
    "@vueuse/core": "^9.0.2",
    "@vueuse/integrations": "^9.0.2",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "element-ui": "^2.15.9",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "normalize.css": "8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "6.2.1",
    "pinia": "^2.0.21",
    "qrcode": "^1.5.1",
    "vue": "^2.7.10",
    "vue-i18n": "^8.24.3",
    "vue-router": "^3.2.0",
    "vuedraggable": "^2.24.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitejs/plugin-vue2-jsx": "^1.0.3",
    "autoprefixer": "^10.4.7",
    "mockjs": "^1.1.0",
    "release-it": "^14.6.1",
    "sass": "^1.54.5",
    "vite": "^3.0.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "git": {
      "push": false,
      "tagName": "v${version}",
      "commitMessage": "chore: release v${version}"
    }
  }
}
