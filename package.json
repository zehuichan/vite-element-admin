    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "path-to-regexp": "6.2.1",
    "pinia": "^2.0.35",
    "qrcode": "^1.5.3",
    "vue": "^2.7.14",
    "vue-i18n": "^8.24.3",
    "vue-router": "^3.6.5",
    "vuedraggable": "^2.24.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@unocss/reset": "^0.51.8",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vitejs/plugin-vue2-jsx": "^1.1.0",
